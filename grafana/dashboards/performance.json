{
  "dashboard": {
    "title": "AkiDB Performance Metrics",
    "uid": "akidb-performance",
    "tags": ["akidb", "performance"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 1,
    "refresh": "10s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Request Latency Percentiles",
        "type": "graph",
        "gridPos": {"x": 0, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(akidb_http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "P50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(akidb_http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "P95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(akidb_http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "P99",
            "refId": "C"
          }
        ],
        "yaxes": [
          {
            "format": "s",
            "label": "Latency",
            "logBase": 1
          },
          {
            "format": "short"
          }
        ],
        "xaxis": {
          "mode": "time"
        },
        "lines": true,
        "fill": 0,
        "linewidth": 2,
        "legend": {
          "show": true,
          "values": true,
          "current": true,
          "avg": true,
          "max": true
        }
      },
      {
        "id": 2,
        "title": "Search Latency by Tier",
        "type": "graph",
        "gridPos": {"x": 12, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(akidb_vector_search_duration_seconds_bucket{tier=\"hot\"}[5m]))",
            "legendFormat": "Hot Tier P95",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(akidb_vector_search_duration_seconds_bucket{tier=\"warm\"}[5m]))",
            "legendFormat": "Warm Tier P95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.95, rate(akidb_vector_search_duration_seconds_bucket{tier=\"cold\"}[5m]))",
            "legendFormat": "Cold Tier P95",
            "refId": "C"
          }
        ],
        "yaxes": [
          {
            "format": "s",
            "label": "Latency",
            "logBase": 10
          },
          {
            "format": "short"
          }
        ],
        "xaxis": {
          "mode": "time"
        },
        "lines": true,
        "fill": 1,
        "linewidth": 2,
        "legend": {
          "show": true,
          "values": true,
          "current": true,
          "avg": true,
          "max": true
        },
        "thresholds": [
          {
            "value": 0.025,
            "colorMode": "critical",
            "op": "gt",
            "fill": false,
            "line": true
          }
        ]
      },
      {
        "id": 3,
        "title": "Insert Throughput",
        "type": "graph",
        "gridPos": {"x": 0, "y": 8, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum(rate(akidb_vector_insert_duration_seconds_count[5m]))",
            "legendFormat": "Inserts/sec",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "ops",
            "label": "Inserts per second"
          },
          {
            "format": "short"
          }
        ],
        "xaxis": {
          "mode": "time"
        },
        "lines": true,
        "fill": 1,
        "linewidth": 2,
        "legend": {
          "show": true,
          "values": true,
          "current": true,
          "avg": true,
          "max": true
        }
      },
      {
        "id": 4,
        "title": "S3 Operation Latency",
        "type": "graph",
        "gridPos": {"x": 12, "y": 8, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(akidb_s3_operation_duration_seconds_bucket{operation=\"put\"}[5m]))",
            "legendFormat": "PUT P95",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(akidb_s3_operation_duration_seconds_bucket{operation=\"get\"}[5m]))",
            "legendFormat": "GET P95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.95, rate(akidb_s3_operation_duration_seconds_bucket{operation=\"delete\"}[5m]))",
            "legendFormat": "DELETE P95",
            "refId": "C"
          }
        ],
        "yaxes": [
          {
            "format": "s",
            "label": "Latency"
          },
          {
            "format": "short"
          }
        ],
        "xaxis": {
          "mode": "time"
        },
        "lines": true,
        "fill": 1,
        "linewidth": 2,
        "legend": {
          "show": true,
          "values": true,
          "current": true,
          "avg": true,
          "max": true
        }
      },
      {
        "id": 5,
        "title": "Cache Hit Rate",
        "type": "graph",
        "gridPos": {"x": 0, "y": 16, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "100 * sum(rate(akidb_cache_hits_total[5m])) / (sum(rate(akidb_cache_hits_total[5m])) + sum(rate(akidb_cache_misses_total[5m])))",
            "legendFormat": "Hit Rate %",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "percent",
            "label": "Hit Rate %",
            "max": 100,
            "min": 0
          },
          {
            "format": "short"
          }
        ],
        "xaxis": {
          "mode": "time"
        },
        "lines": true,
        "fill": 1,
        "linewidth": 2,
        "legend": {
          "show": true,
          "values": true,
          "current": true,
          "avg": true
        }
      },
      {
        "id": 6,
        "title": "Query Response Size Distribution",
        "type": "heatmap",
        "gridPos": {"x": 12, "y": 16, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum(rate(akidb_vector_search_duration_seconds_bucket[5m])) by (le)",
            "legendFormat": "{{le}}",
            "refId": "A",
            "format": "heatmap"
          }
        ],
        "dataFormat": "tsbuckets",
        "yAxis": {
          "format": "s"
        },
        "color": {
          "mode": "spectrum",
          "cardColor": "#b4ff00",
          "colorScale": "sqrt",
          "exponent": 0.5,
          "colorScheme": "interpolateSpectral"
        }
      },
      {
        "id": 7,
        "title": "Request Rate by Method",
        "type": "graph",
        "gridPos": {"x": 0, "y": 24, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum(rate(akidb_http_requests_total[5m])) by (method)",
            "legendFormat": "{{method}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "reqps",
            "label": "Requests/sec"
          },
          {
            "format": "short"
          }
        ],
        "xaxis": {
          "mode": "time"
        },
        "lines": true,
        "fill": 1,
        "linewidth": 2,
        "stack": false,
        "legend": {
          "show": true
        }
      },
      {
        "id": 8,
        "title": "Request Rate by Path",
        "type": "graph",
        "gridPos": {"x": 12, "y": 24, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum(rate(akidb_http_requests_total[5m])) by (path)",
            "legendFormat": "{{path}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "reqps",
            "label": "Requests/sec"
          },
          {
            "format": "short"
          }
        ],
        "xaxis": {
          "mode": "time"
        },
        "lines": true,
        "fill": 1,
        "linewidth": 2,
        "legend": {
          "show": true,
          "alignAsTable": true,
          "values": true,
          "current": true,
          "avg": true
        }
      },
      {
        "id": 9,
        "title": "gRPC Request Duration",
        "type": "graph",
        "gridPos": {"x": 0, "y": 32, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(akidb_grpc_request_duration_seconds_bucket[5m]))",
            "legendFormat": "P50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(akidb_grpc_request_duration_seconds_bucket[5m]))",
            "legendFormat": "P95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(akidb_grpc_request_duration_seconds_bucket[5m]))",
            "legendFormat": "P99",
            "refId": "C"
          }
        ],
        "yaxes": [
          {
            "format": "s",
            "label": "Latency"
          },
          {
            "format": "short"
          }
        ],
        "xaxis": {
          "mode": "time"
        },
        "lines": true,
        "fill": 0,
        "linewidth": 2,
        "legend": {
          "show": true,
          "values": true,
          "current": true,
          "avg": true,
          "max": true
        }
      },
      {
        "id": 10,
        "title": "Insert Latency Distribution",
        "type": "graph",
        "gridPos": {"x": 12, "y": 32, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(akidb_vector_insert_duration_seconds_bucket[5m]))",
            "legendFormat": "P50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(akidb_vector_insert_duration_seconds_bucket[5m]))",
            "legendFormat": "P95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(akidb_vector_insert_duration_seconds_bucket[5m]))",
            "legendFormat": "P99",
            "refId": "C"
          }
        ],
        "yaxes": [
          {
            "format": "s",
            "label": "Latency"
          },
          {
            "format": "short"
          }
        ],
        "xaxis": {
          "mode": "time"
        },
        "lines": true,
        "fill": 0,
        "linewidth": 2,
        "legend": {
          "show": true,
          "values": true,
          "current": true,
          "avg": true,
          "max": true
        }
      }
    ]
  }
}
